@model IEnumerable<IS_11_Coursova.Models.TypeLoad>

@{
    ViewBag.Title = "Index";
}

<h2>Type Loads</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TypeLoadID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TypeLoadID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TypeLoadID })
        </td>
    </tr>
}

</table>
